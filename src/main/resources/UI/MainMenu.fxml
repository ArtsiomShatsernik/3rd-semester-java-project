<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="mainMenu" alignment="CENTER" spacing="70.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.MenuController">
    <padding>
        <Insets bottom="35.0" left="20.0" right="20.0" top="30.0" />
    </padding>
   <children>
      <Label prefHeight="62.0" prefWidth="118.0" text="Menu" textAlignment="CENTER">
         <font>
            <Font name="JetBrains Mono Medium" size="46.0" />
         </font></Label>
      <Button fx:id="BuildFileButton" mnemonicParsing="false" onAction="#onBuildFileClicked" prefHeight="54.0" prefWidth="183.0" text="Build File" textAlignment="CENTER">
         <font>
            <Font name="JetBrains Mono Medium" size="24.0" />
         </font></Button>
      <Button fx:id="ComputeButton" layoutX="30.0" layoutY="172.0" mnemonicParsing="false" onAction="#onComputeButtonClick" prefHeight="54.0" prefWidth="183.0" text="Compute" textAlignment="CENTER">
         <font>
            <Font name="JetBrains Mono Medium" size="24.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
   </children>
</VBox>
